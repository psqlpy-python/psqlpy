{
  "name": "@mdit/plugin-attrs",
  "version": "0.8.0",
  "description": "attrs plugin for MarkdownIt",
  "keywords": [
    "markdownit",
    "markdown-it",
    "markdown-it-plugin",
    "attrs"
  ],
  "homepage": "https://mdit-plugins.github.io/attrs.html",
  "bugs": {
    "url": "https://mdit-plugins/mdit-plugins/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdit-plugins/mdit-plugins.git",
    "directory": "packages/attrs"
  },
  "license": "MIT",
  "author": {
    "name": "Mr.Hope",
    "email": "mister-hope@outlook.com",
    "url": "https://mister-hope.com"
  },
  "main": "lib/index.cjs",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "type": "./lib/index.mts",
      "require": "./lib/index.cjs",
      "import": "./lib/index.mjs",
      "default": "./lib/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">= 18"
  },
  "dependencies": {
    "@types/markdown-it": "^13.0.7"
  },
  "peerDependencies": {
    "markdown-it": "^14.0.0"
  },
  "peerDependenciesMeta": {
    "markdown-it": {
      "optional": true
    }
  },
  "devDependencies": {
    "markdown-it": "14.0.0",
    "@mdit/plugin-katex": "0.8.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin esbuild",
    "clean": "rimraf ./lib"
  }
}
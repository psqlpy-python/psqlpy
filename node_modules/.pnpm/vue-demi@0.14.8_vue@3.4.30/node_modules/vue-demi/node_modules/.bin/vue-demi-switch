#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/aleksandrkiselev/Projects/qaspen-python/psqlpy/node_modules/.pnpm/vue-demi@0.14.8_vue@3.4.30/node_modules/vue-demi/bin/node_modules:/Users/aleksandrkiselev/Projects/qaspen-python/psqlpy/node_modules/.pnpm/vue-demi@0.14.8_vue@3.4.30/node_modules/vue-demi/node_modules:/Users/aleksandrkiselev/Projects/qaspen-python/psqlpy/node_modules/.pnpm/vue-demi@0.14.8_vue@3.4.30/node_modules:/Users/aleksandrkiselev/Projects/qaspen-python/psqlpy/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/aleksandrkiselev/Projects/qaspen-python/psqlpy/node_modules/.pnpm/vue-demi@0.14.8_vue@3.4.30/node_modules/vue-demi/bin/node_modules:/Users/aleksandrkiselev/Projects/qaspen-python/psqlpy/node_modules/.pnpm/vue-demi@0.14.8_vue@3.4.30/node_modules/vue-demi/node_modules:/Users/aleksandrkiselev/Projects/qaspen-python/psqlpy/node_modules/.pnpm/vue-demi@0.14.8_vue@3.4.30/node_modules:/Users/aleksandrkiselev/Projects/qaspen-python/psqlpy/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vue-demi-switch.js" "$@"
else
  exec node  "$basedir/../../bin/vue-demi-switch.js" "$@"
fi

#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/aleksandrkiselev/Projects/qaspen-python/psqlpy/node_modules/.pnpm/d3-dsv@3.0.1/node_modules/d3-dsv/bin/node_modules:/Users/aleksandrkiselev/Projects/qaspen-python/psqlpy/node_modules/.pnpm/d3-dsv@3.0.1/node_modules/d3-dsv/node_modules:/Users/aleksandrkiselev/Projects/qaspen-python/psqlpy/node_modules/.pnpm/d3-dsv@3.0.1/node_modules:/Users/aleksandrkiselev/Projects/qaspen-python/psqlpy/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/aleksandrkiselev/Projects/qaspen-python/psqlpy/node_modules/.pnpm/d3-dsv@3.0.1/node_modules/d3-dsv/bin/node_modules:/Users/aleksandrkiselev/Projects/qaspen-python/psqlpy/node_modules/.pnpm/d3-dsv@3.0.1/node_modules/d3-dsv/node_modules:/Users/aleksandrkiselev/Projects/qaspen-python/psqlpy/node_modules/.pnpm/d3-dsv@3.0.1/node_modules:/Users/aleksandrkiselev/Projects/qaspen-python/psqlpy/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/dsv2json.js" "$@"
else
  exec node  "$basedir/../../bin/dsv2json.js" "$@"
fi

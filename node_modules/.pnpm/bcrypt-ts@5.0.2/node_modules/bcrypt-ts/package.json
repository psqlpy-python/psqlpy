{
  "name": "bcrypt-ts",
  "version": "5.0.2",
  "description": "bcrypt written in typescript",
  "keywords": [
    "bcrypt",
    "bcryptjs"
  ],
  "bugs": {
    "url": "https://github.com/Mister-Hope/bcrypt-ts/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Mr.Hope",
    "email": "mister-hope@outlook.com",
    "url": "https://mister-hope.com"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/node.d.mts",
      "browser": "./dist/browser.mjs",
      "node": "./dist/node.mjs",
      "import": "./dist/node.mjs",
      "require": "./dist/node.cjs",
      "default": "./dist/node.cjs"
    },
    "./browser": {
      "types": "./dist/browser.d.mts",
      "import": "./dist/browser.mjs",
      "require": "./dist/browser.cjs",
      "default": "./dist/browser.mjs"
    },
    "./node": {
      "types": "./dist/node.d.mts",
      "import": "./dist/node.mjs",
      "require": "./dist/node.cjs",
      "default": "./dist/node.cjs"
    }
  },
  "main": "./dist/node.cjs",
  "module": "./dist/node.mjs",
  "browser": "./dist/browser.mjs",
  "types": "./dist/node.d.mts",
  "files": [
    "dist"
  ],
  "nano-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.{js,ts}": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "18.6.1",
    "@commitlint/config-conventional": "18.6.2",
    "@rollup/plugin-alias": "5.1.0",
    "@rollup/plugin-replace": "5.0.5",
    "@types/node": "20.11.19",
    "@typescript-eslint/eslint-plugin": "7.0.2",
    "@typescript-eslint/parser": "7.0.2",
    "@vitest/coverage-v8": "1.3.1",
    "commit-and-tag-version": "12.2.0",
    "commitizen": "4.3.0",
    "cz-git": "1.8.0",
    "esbuild": "0.20.1",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "husky": "9.0.11",
    "nano-staged": "0.8.0",
    "prettier": "3.2.5",
    "rimraf": "5.0.5",
    "rollup": "4.12.0",
    "rollup-plugin-dts": "6.1.0",
    "rollup-plugin-esbuild": "6.1.1",
    "typescript": "5.3.3",
    "vite": "5.1.3",
    "vitest": "1.3.1"
  },
  "packageManager": "pnpm@8.15.3",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin esbuild",
    "changelog": " commit-and-tag-version --skip.bump --skip.commit --skip.tag",
    "clean": "rimraf dist",
    "commit": "pnpm git-cz",
    "dev": "rollup -c rollup.config.ts --configPlugin esbuild -w",
    "lint": "pnpm lint:eslint && pnpm lint:prettier",
    "lint:eslint": "eslint --fix --ext .js,.ts src",
    "lint:prettier": "prettier --check --write .",
    "test": "vitest --coverage"
  }
}